{
  "name": "@yuukoyoung/composables",
  "version": "0.3.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "vue composition utility for yuuko-design",
  "keywords": [
    "composition",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yuukoyoung/yuuko-design.git",
    "directory": "packages/composables"
  },
  "bugs": "https://github.com/yuukoyoung/yuuko-design/issues",
  "license": "MIT",
  "author": {
    "name": "yuuko",
    "email": "yuukoyoung@outlook.com"
  },
  "files": [
    "**/dist",
    "CHANGELOG.md"
  ],
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/es/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "build:clean": "rimraf ./dist",
    "prepack": "turbo run build --filter=@yuukoyoung/composables --cwd=../..",
