  "dependencies": {
    "@vueuse/core": "9.1.1",
    "@yuukoyoung/utils": "workspace:*",
    "vue": "3.2.25"
  },
  "devDependencies": {
    "@types/node": "18.7.13",
    "@vitest/coverage-c8": "0.23.1",
    "@vitest/ui": "0.23.1",
    "@yuukoyoung/types": "workspace:*",
    "rimraf": "3.0.2",
    "typescript": "5.0.4",
    "vite": "3.2.0",
    "vite-plugin-dts": "2.3.0",
    "vitest": "0.23.1"
  }
