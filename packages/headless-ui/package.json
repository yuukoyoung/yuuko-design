{
  "name": "@yuukoyoung/headless-ui",
  "version": "0.3.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "headless ui for yuuko-design",
  "keywords": [
    "headless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yuukoyoung/yuuko-design.git",
    "directory": "packages/headless-ui"
  },
  "bugs": "https://github.com/yuukoyoung/yuuko-design/issues",
  "license": "MIT",
  "author": {
    "name": "yuuko",
    "email": "yuukoyoung@outlook.com"
  },
  "files": [
    "**/dist",
    "CHANGELOG.md"
  ],
  "main": "./dist/headless-ui.cjs.js",
  "module": "./dist/headless-ui.es.js",
  "browser": "./dist/headless-ui.umd.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/headless-ui.cjs.js",
      "import": "./dist/headless-ui.es.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && vue-tsc --project tsconfig.build.json",
    "build:clean": "rimraf ./dist",
    "prepack": "turbo run build --filter=@yuukoyoung/headless-ui --cwd=../.."
  },
  "dependencies": {
    "@vue/reactivity": "3.2.25",
    "@yuukoyoung/composables": "workspace:*",
    "@yuukoyoung/utils": "workspace:*",
    "vue": "3.2.25"
  },
  "devDependencies": {
    "@types/node": "18.7.13",
    "@vitejs/plugin-vue": "3.1.2",
    "rimraf": "3.0.2",
    "typescript": "4.5.4",
    "vite": "3.2.0",
    "vue-tsc": "1.0.5"
  }
}
