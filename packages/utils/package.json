{
  "name": "@yuukoyoung/utils",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "utility for yuuko-design",
  "keywords": [
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yuukoyoung/yuuko-design.git",
    "directory": "packages/utils"
  },
  "bugs": "https://github.com/yuukoyoung/yuuko-design/issues",
  "license": "MIT",
  "author": {
    "name": "yuuko",
    "email": "yuukoyoung@outlook.com"
  },
  "files": [
    "**/dist",
    "CHANGELOG.md"
  ],
  "main": "./dist/utils.cjs.js",
  "module": "./dist/utils.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/utils.cjs.js",
      "import": "./dist/utils.es.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "vite build && tsc --project ./tsconfig.build.json",
    "prepack": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "18.7.13",
    "rimraf": "3.0.2",
    "typescript": "4.5.4",
    "vite": "2.9.9"
  }
}
